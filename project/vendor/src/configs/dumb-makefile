
DUMB_SRC := \
	src/it/itload.c src/it/itload2.c src/it/itmisc.c src/it/itorder.c \
	src/it/itread.c src/it/itread2.c src/it/itrender.c src/it/itunload.c \
	src/it/loadmod.c src/it/loadmod2.c src/it/loads3m.c src/it/loads3m2.c \
	src/it/loadxm.c src/it/loadxm2.c src/it/readmod.c src/it/readmod2.c \
	src/it/reads3m.c src/it/reads3m2.c src/it/readxm.c src/it/readxm2.c src/it/xmeffect.c \
	src/core/atexit.c src/core/duhlen.c src/core/duhtag.c src/core/dumbfile.c \
	src/core/loadduh.c src/core/makeduh.c src/core/rawsig.c src/core/readduh.c \
	src/core/register.c src/core/rendduh.c src/core/rendsig.c src/core/unload.c \
	src/helpers/clickrem.c src/helpers/memfile.c src/helpers/resample.c \
	src/helpers/sampbuf.c src/helpers/silence.c src/helpers/stdfile.c

DUMB_OBJS := $(DUMB_SRC:%.c=%.o)

libdumb$(SS_LIBEXT):$(DUMB_OBJS)
	$(SS_CREATELIB)$@  $(DUMB_OBJS)

src/%.o: src/%.c
	$(SS_CC) $< -c $(SS_OBJOUT)$@ -Iinclude

clean:
	rm -f $(DUMB_OBJS)

